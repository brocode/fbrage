    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
