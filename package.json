    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.3",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.64.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.6.1",
    "typescript": "^5.0.0",
